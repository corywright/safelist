<%= error_messages_for 'address' %>

<!--[form:address]-->
<p><label for="address_street">Street</label><br/>
<%= text_area 'address', 'street'  %></p>

<p><label for="address_city">City</label><br/>
<%= text_area 'address', 'city'  %></p>

<p><label for="address_state">State</label><br/>
