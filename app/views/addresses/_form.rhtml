<p><label for="address_zipcode">Zipcode</label><br/>
<%= text_area 'address', 'zipcode'  %></p>

<p><label for="address_country">Country</label><br/>
<%= text_area 'address', 'country'  %></p>

<p><label for="address_phone">Phone</label><br/>
<%= text_area 'address', 'phone'  %></p>
