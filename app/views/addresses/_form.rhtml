<%= error_messages_for 'address' %>

<!--[form:address]-->
<table>
<tr>
	<th class="vertical">Street</th>
	<td><%= text_area 'address', 'street'  %></td>
</tr>
<tr>
	<th class="vertical">City</th>
	<td><%= text_area 'address', 'city'  %></td>
</tr>
<tr>
	<th class="vertical">State</th>
	<td><%= text_area 'address', 'state'  %></td>
</tr>
<tr>
	<th class="vertical">Zipcode</th>
	<td><%= text_area 'address', 'zipcode'  %></td>
</tr>
<tr>
	<th class="vertical">Country</th>
	<td><%= text_area 'address', 'country'  %></td>
</tr>
<tr>
	<th class="vertical">Phone</th>
	<td><%= text_area 'address', 'phone'  %></td>
</tr>
</table>


